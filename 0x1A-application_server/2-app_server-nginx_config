# This command sets up Nginx, a web server software, to respond to incoming requests for the URL route "/airbnb-onepage/" and serve content from the directory or location on the server where the AirBnB_clone_v2 files are stored.
server {
        listen 80 default_server;
        listen [::]:80 default_server;
        server_name 18.233.64.65;
        add_header X-Served-By 48755-web-01;
        location = /airbnb-onepage/ {
                proxy_pass http://127.0.0.1:5000/airbnb-onepage/;
        }
        error_page 404 /404.html;
        location /404 {
                root /var/www.html;
                internal;
        }
}
